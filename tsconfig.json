{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@views/*": ["src/views/*"],
      "@components/*": ["src/components/*"],
      "@data": ["src/data/info"],
      "@utils": ["src/utils"],
      "@types": ["src/types"]
    }
  }
}
